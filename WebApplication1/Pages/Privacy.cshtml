    public class GenerateNumber
    {
    public static List<int>
        Get(int min, int max, int count)
        {
        List<int>
            list = new List<int>
                ();
                Random rnd = new Random();
                int n = rnd.Next(min, max);
                list.Add(n);
                while (list.Count < count)
                {
                n = rnd.Next(min, max);
                if (list.All(x => x != n))
                {
                list.Add(n);
                }
                }
                return list;
                }

                }
